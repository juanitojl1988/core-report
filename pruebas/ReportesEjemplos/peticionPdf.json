{
    "nameReport": "reportePrueba",
    "template": "<style> table { width: 100%; border-collapse: collapse; font-family: Arial, sans-serif; margin-top: 20px; } th, td { border: 1px solid #000; padding: 8px; text-align: left; } th { background-color: #4CAF50; color: white; font-weight: bold; } tr:nth-child(even) { background-color: #f2f2f2; } tr:hover { background-color: #ddd; } .title { text-align: center; font-size: 18px; margin: 10px 0; } .header { font-size: 14px; margin: 5px 0; font-weight: normal; } </style> <table> <thead> <tr> <th colspan='3' class='title'>Reporte de Usuarios</th> </tr> <tr> <th class='header'>Fecha de Generación:</th> <td class='header' colspan='2'>{{one.generationdate}}</td> </tr> <tr> <th class='header'>Generado por:</th> <td class='header' colspan='2'>{{one.generatedby}}</td> </tr> <tr> <th class='header'>Estación:</th> <td class='header' colspan='2'>{{one.station}}</td> </tr> <tr> <td colspan='3' style='height: 10px;'></td> <!-- Fila de separación --> </tr> <tr> <th>ID</th> <th>Nombre</th> <th>Email</th> </tr> </thead> <tbody> {{#each list}} <tr> <td>{{this.id}}</td> <td>{{this.name}}</td> <td>{{this.email}}</td> </tr> {{/each}} </tbody> </table>",
    "query": {
        "one": {
             "parameters": {
                "cagencia": 1,
                "cusuarios": [
                    1,
                    2,
                    3,
                    4,
                    5,
                    6
                ],
                "fecha_registro": "2023-09-01",
                "estado": "activo"
            },
            "sql": "SELECT 1 AS generationDate, 'Juan' AS generatedBy, 'juan@example.com' AS station",
            "sqlCount": ""
        },
        "list": {
             "parameter": {
                "cagencia": 1,
                "cusuarios": [
                    1,
                    2,
                    3,
                    4,
                    5,
                    6
                ],
                "fecha_registro": "2023-09-01",
                "estado": "activo"
            },
            "sql": "select nombre_usuario , fecha_registro , cusuario , 100.00 valor, a.ciudad  from usuario u inner join agencia a on u.cagencia  = a.cagencia  where u.cagencia = :cagencia and u.cusuario  in (1,2,3,4,5,6) and u.fecha_registro =:fecha_registro and u.estado = :estado",
            
            
            "sqlCount": "select count(*)  from usuario u inner join agencia a on u.cagencia  = a.cagencia  where u.cagencia = :cagencia and u.cusuario  in (1,2,3,4,5,6) and u.fecha_registro =:fecha_registro and u.estado = :estado"
        }
    },
